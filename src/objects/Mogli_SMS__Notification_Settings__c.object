        <externalId>false</externalId>
        <label>Allow Notifications For</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Any incoming SMS</fullName>
                    <default>true</default>
                    <label>Any incoming SMS</label>
                </value>
                <value>
                    <fullName>Only incoming SMS from a Contact I own</fullName>
                    <default>false</default>
                    <label>Only incoming SMS from a Contact I own</label>
                </value>
                <value>
                    <fullName>Only incoming SMS from a Lead I own</fullName>
                    <default>false</default>
                    <label>Only incoming SMS from a Lead I own</label>
                </value>
                <value>
                    <fullName>Only incoming SMS from Contacts and Leads I own</fullName>
                    <default>false</default>
                    <label>Only incoming SMS from Contacts and Leads I own</label>
                </value>
                <value>
                    <fullName>Do not show any SMS notifications</fullName>
                    <default>false</default>
                    <label>Do not show any SMS notifications</label>
                </value>
            </valueSetDefinition>
        </valueSet>
