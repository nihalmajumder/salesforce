    <fields>
        <fullName>Case_Age_Business_Hours__c</fullName>
        <description>Used in SSR Reports and Dashboards</description>
        <externalId>false</externalId>
        <formula>max( 0, 


( 

if( text(Status) = &quot;Closed&quot;, 



((ClosedDate - CreatedDate) * 24) 

- 

( (datevalue(ClosedDate) - datevalue(CreatedDate)) * 12 ) 

- 

( FLOOR((datevalue(ClosedDate) - datevalue(CreatedDate) ) / 7) * 24 ) 

- 

(if(((MOD( datevalue(ClosedDate) - DATE(1900, 1, 6), 7)) - mod((datevalue(ClosedDate) - datevalue(CreatedDate)), 7)) &lt; 2, 24, 0)) 

, 

((now() - CreatedDate) * 24) 

- 

( (today() - datevalue(CreatedDate)) * 12 ) 

- 

( FLOOR((today() - datevalue(CreatedDate) ) / 7) * 24 ) 

- 

(if(((MOD( today() - DATE(1900, 1, 6), 7)) - mod((today() - datevalue(CreatedDate)), 7)) &lt; 2, 24, 0)) 

) 

) 

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Case Age - Business Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
